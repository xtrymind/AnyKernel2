# Everything here won't interfere with qcom-post-boot
on property:sys.boot_completed=1
# disable iostat
    write /sys/block/mmcblk0/queue/iostats 0
    write /sys/block/mmcblk1/queue/iostats 0

# Must be executed after qcom-post-boot
on property:init.svc.qcom-post-boot=stopped
# configure governor settings for little cluster
    write /sys/devices/system/cpu/cpu0/online 1
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay "60000 902400:75000 1401600:100000"
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load 95
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/hispeed_freq 902400
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads "63 902400:73 1113600:72 1401600:82 1536000:86 1612800:79"
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/min_sample_time 18000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_rate 20000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_slack 180000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/use_sched_load 1
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/ignore_hispeed_on_notif 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/enable_prediction 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/boostpulse_duration 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/boost 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/use_migration_notif 1
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/io_is_busy 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/max_freq_hysteresis 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/fast_ramp_down 1

# configure governor settings for big cluster
    write /sys/devices/system/cpu/cpu4/online 1
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load 95
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/hispeed_freq 1401600
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay "32000 1113600:60000 1401600:75000 1747200:80000"
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads "72 1401600:72 1747200:87 1804800:76"
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/timer_rate 20000
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/min_sample_time 18000
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/timer_slack 180000
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/use_sched_load 1
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/ignore_hispeed_on_notif 0
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/enable_prediction 0
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/use_migration_notif 1
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/io_is_busy 0
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/max_freq_hysteresis 59000
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/fast_ramp_down 1

# Switch to BFQ I/O scheduler
    setprop sys.io.scheduler bfq
